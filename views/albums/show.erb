<h1><%= @album.musicians.join(' / ') %></h1>
<img class="album" src="<%= @album.image_path %>" alt="<%= @album.title %>">
<h1><%= @album.title %> (<%= @album.year %>)</h1>
<ul>
	<% @songs.each do |song| %>
		<li>
			<%= song.track_num %>. <%= song.title %>  <%= song.time %>
		</li>
	<% end %>
</ul>
